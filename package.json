{
  "name": "tinkertank-market",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "format": "prettier --write .",
    "type-check": "tsc --noEmit",
    "test": "vitest",
    "db:generate": "npx prisma generate",
    "db:migrate": "npx prisma migrate dev",
    "db:reset": "npx prisma migrate reset",
    "db:seed": "npx prisma db seed",
    "db:studio": "npx prisma studio",
    "db:setup": "tsx scripts/db-setup.ts",
    "db:reset-dev": "tsx scripts/db-reset.ts",
    "db:backup": "tsx scripts/db-backup.ts backup",
    "db:restore": "tsx scripts/db-backup.ts restore",
    "db:backups": "tsx scripts/db-backup.ts list",
    "db:status": "tsx scripts/db-status.ts",
    "db:validate": "tsx scripts/db-validate.ts",
    "test:deployment": "tsx scripts/test-deployment.ts",
    "check:deployment": "tsx scripts/simple-deployment-check.ts",
    "check:ready": "tsx scripts/production-readiness.ts",
    "test:integration": "vitest run src/test/integration",
    "test:accessibility": "vitest run src/test/accessibility",
    "test:mobile": "vitest run src/test/mobile",
    "test:a11y-mobile": "vitest run src/test/accessibility src/test/mobile",
    "test:performance": "vitest run performance/tests --reporter=verbose",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:cart": "vitest run src/__tests__/unit src/__tests__/integration",
    "test:coverage": "vitest run --coverage",
    "test:stripe": "tsx scripts/test-stripe-payment.ts",
    "test:complete-payment": "tsx scripts/complete-payment-test.ts",
    "perf:all": "node performance/scripts/run-performance-suite.js",
    "perf:lighthouse": "node performance/scripts/lighthouse-audit.js",
    "perf:memory": "node performance/scripts/memory-profiler.js",
    "perf:load": "npx artillery run performance/config/artillery.yml",
    "perf:budget": "node performance/scripts/lighthouse-audit.js budget",
    "precommit": "npm run lint && npm run type-check && npm run test -- --run"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  },
  "dependencies": {
    "@fullcalendar/core": "^6.1.19",
    "@fullcalendar/daygrid": "^6.1.15",
    "@fullcalendar/interaction": "^6.1.15",
    "@fullcalendar/react": "^6.1.15",
    "@fullcalendar/timegrid": "^6.1.19",
    "@headlessui/react": "^2.2.7",
    "@heroicons/react": "^2.2.0",
    "@prisma/client": "^6.15.0",
    "@stripe/react-stripe-js": "^2.8.1",
    "@stripe/stripe-js": "^4.8.0",
    "clsx": "^2.1.1",
    "date-fns": "^3.6.0",
    "date-fns-tz": "^3.2.0",
    "next": "^16.0.1",
    "prisma": "^6.15.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "stripe": "^17.3.1",
    "tailwind-merge": "^2.5.4",
    "zod": "^4.1.5",
    "zustand": "^4.4.7"
  },
  "devDependencies": {
    "@playwright/test": "^1.40.1",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.4.8",
    "@testing-library/react": "^16.0.1",
    "@testing-library/user-event": "^14.5.2",
    "@types/benchmark": "^2.1.5",
    "@types/node": "^20.16.10",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@vitejs/plugin-react": "^4.3.2",
    "artillery": "^2.0.24",
    "autoprefixer": "^10.4.20",
    "eslint": "^8.57.1",
    "eslint-config-next": "15.0.3",
    "jest-axe": "^8.0.0",
    "jsdom": "^25.0.1",
    "lighthouse": "^12.8.2",
    "msw": "^2.0.11",
    "postcss": "^8.4.49",
    "prettier": "^3.3.3",
    "tailwindcss": "^3.4.14",
    "tsx": "^4.20.5",
    "typescript": "^5.6.3",
    "vitest": "^2.1.4"
  }
}
